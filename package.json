{
    "name": "gamescript",
    "version": "1.0.0",
    "description": "An Electron application with Svelte and TypeScript",
    "main": "./out/main/index.js",
    "author": "example.com",
    "homepage": "https://electron-vite.org",
    "scripts": {
        "format": "prettier --plugin prettier-plugin-svelte --write .",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "svelte-check": "",
        "typecheck": "npm run typecheck:node && npm run svelte-check",
        "start": "electron-vite preview",
        "dev": "electron-vite dev",
        "build": "npm run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:win": "npm run build && electron-builder --win --config",
        "build:mac": "npm run build && electron-builder --mac --config",
        "build:linux": "npm run build && electron-builder --linux --config",
        "test": "vitest"
    },
    "dependencies": {
        "@electron-toolkit/utils": "^3.0.0",
        "better-sqlite3": "^9.4.5"
    },
    "devDependencies": {
        "@ag-grid-community/core": "^31.2.0",
        "@ag-grid-community/infinite-row-model": "^31.2.0",
        "@ag-grid-community/styles": "^31.2.0",
        "@ag-grid-enterprise/column-tool-panel": "^31.2.0",
        "@ag-grid-enterprise/core": "^31.2.0",
        "@ag-grid-enterprise/filter-tool-panel": "^31.2.0",
        "@ag-grid-enterprise/side-bar": "^31.2.0",
        "@electron-toolkit/eslint-config-prettier": "^2.0.0",
        "@electron-toolkit/eslint-config-ts": "^1.0.1",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@types/better-sqlite3": "^7.6.9",
        "@types/node": "^18.19.29",
        "@types/pg": "^8.11.4",
        "@types/stream-json": "^1.7.7",
        "@xyflow/svelte": "^0.0.40",
        "antlr4": "4.13.1-patch-1",
        "carbon-components-svelte": "^0.82.8",
        "carbon-icons-svelte": "^12.6.0",
        "carbon-pictograms-svelte": "^12.10.0",
        "carbon-preprocess-svelte": "^0.10.0",
        "csv-parse": "^5.5.5",
        "csv-stringify": "^6.4.6",
        "electron": "^29.2.0",
        "electron-builder": "^24.13.3",
        "electron-vite": "^2.1.0",
        "eslint": "^8.57.0",
        "eslint-plugin-svelte": "^2.35.1",
        "monaco-editor": "^0.46.0",
        "mousetrap": "^1.6.5",
        "pg": "^8.11.5",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.2",
        "stream-json": "^1.8.0",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.9",
        "tslib": "^2.6.2",
        "typescript": "^5.4.4",
        "vite": "^5.2.8",
        "vitest": "^1.4.0"
    }
}
